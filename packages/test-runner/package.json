{
    "name": "@flowtr/test",
    "version": "0.0.1",
    "description": "Yet another test runner written in typescript",
    "type": "module",
    "main": "./dist/index.cjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.mjs",
            "require": "./dist/index.cjs"
        }
    },
    "bin": {
        "fltest": "./dist/cli.mjs"
    },
    "size-limit": [
        {
            "path": "dist/index.mjs"
        }
    ],
    "scripts": {
        "cm": "cz",
        "build": "rollup -c",
        "size": "size-limit",
        "lint": "eslint src/**/*.ts --fix",
        "prepublishOnly": "pnpm run lint && pnpm run test && pnpm run build"
    },
    "keywords": [
        "typescript",
        "node"
    ],
    "author": "Theo Paris",
    "license": "MIT",
    "devDependencies": {
        "@commitlint/cli": "^13.1.0",
        "@commitlint/config-conventional": "^13.1.0",
        "@rollup/plugin-commonjs": "^20.0.0",
        "@rollup/plugin-node-resolve": "^13.0.4",
        "@types/node": "16.4.12",
        "rollup": "^2.55.1",
        "rollup-plugin-dts": "^3.0.2",
        "rollup-plugin-typescript2": "^0.30.0",
        "tslib": "^2.3.0",
        "typescript": "4.3.5"
    },
    "dependencies": {
        "@flowtr/logger": "workspace:*",
        "bandersnatch": "^1.7.1",
        "esbuild": "^0.12.17",
        "esbuild-runner": "^2.2.0",
        "fast-deep-equal": "^3.1.3",
        "tiny-glob": "^0.2.9",
        "vm2": "^3.9.3"
    }
}
